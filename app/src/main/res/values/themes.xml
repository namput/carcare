<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Newcar" parent="Theme.AppCompat.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colorAccent</item>
        <item name="colorPrimaryVariant">#004d81</item>
        <item name="colorOnPrimary">@color/colorPrimaryDark</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/md_teal_200</item>
        <item name="colorSecondaryVariant">@color/md_teal_700</item>
        <item name="colorOnSecondary">@color/md_black_1000</item>
        <item name="android:fontFamily">@font/mitrregular</item>
        <item name="android:textColor">@color/md_amber_700</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>